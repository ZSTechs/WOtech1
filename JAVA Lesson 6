ublic class Main {
public static void main(String[] args) {
  // System.out.println("Hello world!");
  // winter, spring, summer, autumn
  // warm jacket, t-shirt, swimming suite, rain coat
  String season = "qwerty";

  if (season == "winter") {
    System.out.println("Wear a warm jacket!");
  } else if (season == "spring") {
    System.out.println("Wear a T-shirt!");
  } else if (season == "summer") {
    System.out.println("Wear a swimming suite!");
  } else if (season == "autumn") {
    System.out.println("Wear a rain coat!");
  } else {
    System.out.println("I do not recongnize this season!");
  }

  double temp = -10.23;

  if (temp <= 5) {
    System.out.println("Wear super warm");
  } else if (temp <= 15) {
    System.out.println("Wear warm");
  } else if (temp <= 30) {
    System.out.println("Wear normal");
  } else {
    System.out.println("You need cooling");
  }

}
}

public class Main {
  static int sharedValue = 200; // vienmēr "static"
  
  public static void main(String[] args){  // uniq entry point
    int mainValue =14;
    System.out.println(mainValue);
    firstMethod();
    secondMethod();
    //authenticateUser();
    //verifyUser();
    //checkUser();
    //prepareMessage();
    //sendEmail();
    //sendSms();
    //registerInDatabase();

    //katra metode nosaka tikai vienu funkciju, bet "Main" tās izsauc.  
  }

  public static void firstMethod(){
  int firstValue = 50;
    System.out.println(firstValue);
  }

  public static void secondMethod(){
int secValue = 100;
    System.out.println(secValue);
  }  
}

```java
import java.util.Scanner;  
public class Main {
  public static void main(String[] args) {
    Scanner user = new Scanner(System.in);  
    System.out.println("Enter your name");
    String userName = user.nextLine();  
    System.out.println("Hi, " + userName);  

    Scanner age = new Scanner(System.in);
    System.out.println("What is your age?");
    String ageName = age.nextLine();

    Scanner proffesion = new Scanner(System.in);  
    System.out.println("What is your proffesion?");
    String proffesionName = proffesion.nextLine();
    
    Scanner season = new Scanner(System.in);
    System.out.println("What is your favorite season of the year?");
    String seasonName = season.nextLine();
    
    Scanner hobby = new Scanner(System.in);
    System.out.println("Enter hobby");
    String hobbyName = hobby.nextLine();

System.out.println(userName+ " ,thank you for answering my questions, now I know you are a " +ageName+ " years old "+ proffesionName + " who likes " + seasonName+ " and loves to " + hobbyName+ ".");
  }
}
```
